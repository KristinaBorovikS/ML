students = {
    "student1": {"name": "Anna", "age": 20 ,"course" : 3},
    "student2": {"name": "Pavel", "age": 19, "course" : 2},
    "student3": {"name": "Maria", "age": 17, "course" : 1}
}

for user, details in students.items():
   if (details.get('name') == "Anna") :
       for key, value in details.items(): print(key,":", value)

